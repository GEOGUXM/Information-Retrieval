   [Application File] START /spo/prodlist.taf alltype=3
   [Results Action] Results
   [Changed Vars] user$type=3
   [Search Action] Search
   [Query] SELECT
   C1.OFFERING_CODE,C1.DATA_DISPLAY_NAME,C1.ON_LINE,C1.OFFERING_TYPE,C1.OR
   GANIZATIONAL_UNIT,C1.FIRST_YEAR FROM ORCA_LIB.CATALOG C1 WHERE
   (C1.OFFERING_TYPE=3 AND C1.ORGANIZATIONAL_UNIT=:v1) ORDER BY 6 DESC,1
   ASC
   [BoundVals] [v1='SEA']
   [ActionResults] 3_SEA_99-03 Counties Geographic Information System
   (GIS) Files Y 3 SEA 1999
   [Changed Vars] user$pastyear=1960
   [user$ Vars] pastyear=1960 type=3 variabletimeout=240
   [local$ Vars] resultset=[Array:15x6] variabletimeout=240
